pages = {72–77},
numpages = {6},
keywords = {Artificial intelligence, English education, Personalized learning},
location = {Shanghai, China},
series = {EKI '24}
}

@inproceedings{10.1145/3697355.3697411,
author = {Min, Yuan and Huang, Zhong},
