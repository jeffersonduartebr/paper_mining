doi = {10.1145/3706890.3706895},
abstract = {Recent advancements in artificial intelligence (AI) have significantly impacted various medical fields, including plastic and reconstructive surgery. This review aims to explore cutting-edge developments in AI applications for peripheral nerve repair, a critical area in restorative surgery due to its complex nature and high demands for precision. The integration of AI techniques, such as machine learning algorithms and neural networks, has enhanced diagnostic accuracy, prognostic evaluations, and surgical outcomes in peripheral nerve injuries. AI-driven models facilitate the identification of optimal surgical approaches, predict patient-specific recovery trajectories, and assist in real-time intraoperative decision-making, thereby improving the overall success rates of nerve repair procedures. Additionally, innovative AI tools have been developed to simulate nerve regeneration and functional recovery, providing valuable insights for preoperative planning and postoperative rehabilitation. This review highlights key technological advancements, discusses their clinical implications, and identifies potential future directions for research. By harnessing the power of AI, the field of peripheral nerve repair stands poised to achieve unprecedented levels of precision and efficacy, ultimately enhancing patient outcomes and quality of life.},
booktitle = {Proceedings of the 2024 5th International Symposium on Artificial Intelligence for Medicine Science},
pages = {27–34},
numpages = {8},
keywords = {Artificial intelligence, Clinical Management, Peripheral nerve repair},
location = {
},
series = {ISAIMS '24}
}

