title = {Research on online learning behavior classification method based on LSTM and CNN},
year = {2024},
isbn = {9798400707100},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3700297.3700314},
doi = {10.1145/3700297.3700314},
abstract = {Online education has become increasingly prominent in the 21st century, a trend further accelerated by recent global events. While online platforms offer flexibility and convenience, they also present challenges in monitoring and evaluating student behavior and learning outcomes. Traditional data analysis methods often struggle with the vast, complex, and unstructured data generated by these platforms. This study proposes a method based on deep learning, specifically utilizing Long Short-Term Memory (LSTM) networks and Convolutional Neural Networks (CNN). These models process time series and multi-dimensional behavior data, extracting key features to classify student learning behaviors into active participation, passive participation, and irregular participation. The proposed method demonstrates superior performance over traditional methods in terms of accuracy, precision, recall, and F1-score, validating its effectiveness in classifying online learning behaviors. The implications for educational theories and pedagogies are significant, providing a foundation for developing more responsive and supportive online learning environments.},
booktitle = {Proceedings of the 2024 International Symposium on Artificial Intelligence for Education},
pages = {94–100},
numpages = {7},
keywords = {Convolutional neural network, Learning behavior analysis, Long short-term memory network, Online education},
location = {
