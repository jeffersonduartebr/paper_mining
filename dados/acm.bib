@inproceedings{10.1145/3494193.3494196,
author = {Papadopoulos, Theodoros and Charalabidis, Yannis},
title = {Developing a National Strategy for Artificial Intelligence: The case of Greece: This document presents and discusses the development and the contents of the Greek national AI strategy},
year = {2022},
isbn = {9781450390118},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
