isbn = {9798400710650},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3708394.3708414},
doi = {10.1145/3708394.3708414},
abstract = {The design of English teaching system based on artificial intelligence technology aims to solve the problems of insufficient training time and low efficiency in traditional English teaching. The research adopts advanced technologies such as intelligent speech recognition, natural language processing and machine learning to build an efficient and interactive learning environment. By integrating virtual reality and augmented reality technology, the system strengthens students' language application ability and encourages communication in simulated or actual contexts. The classroom teaching mode covers three stages: before class, during class and after class, to ensure the comprehensive improvement of students' listening and speaking skills. ITS module design includes student model, teacher model and expert model, which realizes the adjustment of personalized teaching content, evaluates learning results and provides targeted solutions. The realization of system functions depends on three core components: remote courseware service system, customer mobile learning system and data agent system, providing a stable and efficient personalized adaptive learning experience. The test results show that the system can still maintain high efficiency and stability when the number of concurrent users increases, which is superior to the traditional teaching system. In addition, intelligent composition correction, oral practice feedback, personalized learning plan recommendation and intelligent translation tools have significantly improved students' English writing ability and oral fluency.},
booktitle = {Proceeding of the 2024 International Conference on Artificial Intelligence and Future Education},
pages = {109–113},
numpages = {5},
keywords = {English, artificial intelligence technology, teaching system},
location = {
},
series = {AIFE '24}
