@ARTICLE{Branny2025667,
	author = {Branny, Artur and Maurer, Megan and Andersson, Erik and McPhearson, Timon and Raymond, Christopher M. and Faehnle, Maija and Olafsson, Anton Stahl and Gulsrud, Natalie Marie},
	title = {Correction to: Introducing listening as a weak method for advancing sustainability and interdisciplinary scholarship (Sustainability Science, (2025), 20, 1, (77-94), 10.1007/s11625-024-01571-w)},
	year = {2025},
	journal = {Sustainability Science},
