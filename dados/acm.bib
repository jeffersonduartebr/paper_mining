abstract = {The first time I recall encountering artificial intelligence was in the early 2000s while working in a recording studio. After singing a take of a song, I watched as an engineer opened a plug-in called Auto-tune and then listened as he worked on tuning my vocals. I recall him explaining to me that the vocal had to be pretty close to the note I was trying to sing, otherwise the tuned version would sound fake. He demonstrated by tuning my vocal too sharp and then too flat. The sound of the stressed vocal created a distorted tremolo effect, with maybe even a bit of delay. It no longer sounded like me. It sounded like a robot who was impersonating me. I bristled. "No, not like that," I said uncomfortably laughing. "That doesn't sound like me."},
journal = {Commun. Des. Q. Rev},
month = jan,
pages = {50–52},
numpages = {3},
keywords = {artificial intelligence, authenticity, writing}
}

@inproceedings{10.1145/3695652.3695714,
