doi = {10.1145/3670013.3670066},
abstract = {Online course design in higher education continues to be a challenge. Complex and rich data require scalable research methods. Conflicting goals and aims of Massive Open Online Courses (MOOC) complicate the research field. To understand educationally beneficial components of online courses, multiple factors and viewpoints have to be taken into consideration. A particular focus of this research is to understand the expectation and consumption experience of university student, and how their satisfaction and dissatisfaction inform online course design. This paper looks at questionnaire results gathered from undergraduate students before and after taking a 15-week (one-semester) online course about algorithms and programming. The questionnaire was planned to measure student satisfaction and dissatisfaction of 12 features related to online course design using the Kano model. The questionnaire also included freeform text comments to gain insight into students’ expectation and consumption experience. A Word2Vec language model was trained on the freeform questions. Keywords related to the 12 online course features were analyzed using the language model. The results substantiate the use of specialized language models to support the Kano model analysis.},
booktitle = {Proceedings of the 2024 15th International Conference on E-Education, E-Business, E-Management and E-Learning},
pages = {183–188},
numpages = {6},
keywords = {artificial intelligence, e-learning, kano model, online course, user satisfaction, word embeddings},
