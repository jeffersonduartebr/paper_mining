doi = {10.1145/3700297.3700376},
abstract = {With the rapid advancement of big data and artificial intelligence technologies, the application of data analysis in education has deepened considerably. This paper specifically examines the application of the K-means clustering algorithm in analyzing the scores of the College English Test Band 4. Utilizing clustering techniques, this paper conducts an in-depth analysis of CET-4 examination score data, thereby identifying the distribution patterns and characteristics of examinees at different performance levels. Additionally, it explores the differences among these groups in terms of examination scores, learning strategies, habits, and other relevant factors. The research results reveal the underlying patterns of CET-4 score distribution and provides a scientific basis for formulating both personalized teaching strategies and learning plans, ultimately contributing to improved teaching effectiveness and student learning outcomes.},
booktitle = {Proceedings of the 2024 International Symposium on Artificial Intelligence for Education},
