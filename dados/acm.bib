@inproceedings{10.1145/3647444.3647881,
author = {Kumar, Vani and Bhardwaj, Vanshaj and Anbarasu, V},
title = {Credit Card Fraud Detector for Lower Ranged Transactions using AI Algorithms},
year = {2024},
