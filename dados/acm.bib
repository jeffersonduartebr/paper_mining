title = {Synergizing Optogenetics and Artificial Intelligence: A Novel Paradigm for Advanced Neuroscience InvestigationsAI and Optogenetics: Advancing Neuroscience},
year = {2024},
isbn = {9798400708138},
publisher = {Association for Computing Machinery},
