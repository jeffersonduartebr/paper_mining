@inproceedings{10.1145/3454127.3457618,
author = {Bouchareb, Asmae and Boulaalam, Abdelhak and Bellamine, Insaf},
title = {IoT and AI based intelligent system to fight against COVID-19},
year = {2021},
isbn = {9781450388719},
