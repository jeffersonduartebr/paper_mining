@inproceedings{10.1145/3687311.3687389,
author = {Liu, Qian and Li, Yanjuan and Zhang, Kun},
title = {Application Research of Learning Path Recommendation in Skills Development of Vocational College Students},
year = {2024},
isbn = {9798400709920},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3687311.3687389},
doi = {10.1145/3687311.3687389},
abstract = {The objective of this study is to explore the application effects of learning path recommendation in skill development among vocational college students. By integrating vocational education and artificial intelligence technology, a personalized learning path recommendation model is designed, and a corresponding recommendation system is developed. Student information is collected, and competency assessments are conducted to provide accurate learning path recommendations. Students can choose suitable learning resources and courses based on their individual needs and learning progress. The introduction of personalized learning path recommendation has a positive impact on students' skill development. The learning path recommendation system provides personalized learning support to students, enhancing learning outcomes and motivation. By selecting appropriate learning resources and courses, students can apply their acquired knowledge to real-world scenarios, improving their problem-solving abilities and professional competence. Learning path recommendation holds significant application value in skill development among vocational college students. The personalized learning path recommendation system facilitates students' autonomous learning abilities and personal development by providing accurate recommendations on learning resources and courses. Future research can further explore the precision and flexibility of learning path recommendation systems, as well as their integration with practical projects and real-life cases, to further enhance skill development among vocational college students. The application of learning path recommendation systems will provide vocational colleges with more effective educational tools, assisting students in achieving personal development and professional success.},
booktitle = {Proceedings of the 2024 International Conference on Intelligent Education and Computer Technology},
pages = {431–435},
numpages = {5},
location = {Guilin, China},
series = {IECT '24}
}

@article{10.1145/3725527,
author = {Aljohani, Abdulaziz and Rana, Omer and Perera, Charith},
title = {Self-adaptive Federated Learning in Internet of Things Systems: A Review},
year = {2025},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
issn = {0360-0300},
url = {https://doi.org/10.1145/3725527},
doi = {10.1145/3725527},
abstract = {In recent years, Federated Learning (FL) and the Internet of Things (IoT) have enabled numerous Artificial Intelligence (AI) applications. FL offers advantages over traditional Machine Learning (ML) and Deep Learning (DL) by shifting model training to the edge. However, the dynamic nature of IoT environments often interferes with FL’s ability to converge quickly and deliver consistent performance. Therefore, a self-adaptive approach is necessary to react to context changes and maintain system performance. This paper provides a systematic overview of current efforts to integrate self-adaptation in FL for IoT. We review key computing disciplines, including Self-Adaptive Systems (SAS), Feedback Controls, IoT, and FL. Additionally, we present (i) a multidimensional taxonomy to highlight the core characteristics of self-adaptive FL systems and (ii) a conceptual architecture for self-adaptive FL in IoT, applied to Anomaly Detection (AD) in smart homes. Finally, we discuss the motivations, implementations, applications, and challenges of self-adaptive FL systems in IoT contexts.},
note = {Just Accepted},
journal = {ACM Comput. Surv.},
month = mar,
keywords = {IoT, Smart Home, Federated Learning, Resilience, Adaptation}
}

@inproceedings{10.1145/3641555.3705276,
author = {Qian, Yingxiao and Tang, Hengtao},
title = {Untangling School Leaders' Perception of AI Integration in K-12 Education},
year = {2025},
isbn = {9798400705328},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3641555.3705276},
doi = {10.1145/3641555.3705276},
abstract = {Artificial intelligence (AI) is transforming education, with the potential to revolutionize teaching, learning, and school administration. However, the successful implementation of AI-driven initiatives involves more than simply adopting new technology but requires a thorough understanding of how school leaders perceive AI and their preparedness to integrate it into educational planning and practices. Principals and school leaders play a critical role in supporting effective instruction and student achievement across different content areas, but their needs and challenges, particularly regarding AI integration, are often overlooked. Understanding school leaders' perceptions of AI is vital for successfully implementing AI in schools. This exploratory study employed a qualitative approach to investigate school leaders' perspectives on AI integration in K-12 school settings. Two 90-minute semi-structured focus groups were conducted. Inductive analysis was performed to interpret the qualitative data. The findings highlighted school leaders' perceptions of best practices of AI integration as well as their concerns about AI and the associated ethical dilemma. This collective knowledge of potential solutions for AI integration can help establish a systematic, top-down approach in K-12 school settings.},
booktitle = {Proceedings of the 56th ACM Technical Symposium on Computer Science Education V. 2},
pages = {1593–1594},
numpages = {2},
keywords = {K-12 education, artificial intelligence, educational leadership, qualitative study},
location = {Pittsburgh, PA, USA},
series = {SIGCSETS 2025}
}

