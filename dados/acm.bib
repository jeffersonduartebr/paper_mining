author = {Wilchek, Matthew and Wang, Linhan and Dickinson, Sally and Feuerbacher, Erica and Luther, Kurt and Batarseh, Feras A.},
title = {KHAIT: K-9 Handler Artificial Intelligence Teaming for Collaborative Sensemaking},
year = {2025},
isbn = {9798400713064},
