address = {New York, NY, USA},
url = {https://doi.org/10.1145/3656766.3656867},
doi = {10.1145/3656766.3656867},
abstract = {The Intelligent Internet of Things (AIoT), merging artificial intelligence with advanced IoT technologies, has been deeply implemented in domains such as smart cities, intelligent homes, intelligent transportation, and vehicular networks. This technological advancement brings unprecedented convenience, yet it also introduces new challenges in data privacy management. With the surge in data volume and intensifying regulatory demands, enterprises are compelled to seek more scientific and efficient privacy management solutions to maintain continuous compliance and competitiveness. This paper presents an innovative privacy management framework based on a comprehensive analysis of theoretical models such as the ISO29100 Privacy Framework, NIST Privacy Framework, and ISO27701 Privacy Information Management System. It employs the methodology of ISO31000 Risk Management and leverages the structural foundation of the ISO27001 Information Security Management System. This framework aims to integrate industry best practices, tightly coupling privacy engineering principles with the system design process. It assists organizations in thoroughly identifying, assessing, managing, and mitigating privacy risks, ensuring strict privacy protection standards are met throughout the lifecycle of data from collection, storage, processing to destruction.},
booktitle = {Proceedings of the 2023 3rd International Conference on Big Data, Artificial Intelligence and Risk Management},
pages = {599–608},
numpages = {10},
location = {Chengdu, China},
series = {ICBAR '23}
