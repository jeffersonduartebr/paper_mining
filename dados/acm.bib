@article{10.1145/3644369,
author = {Zhou, Ziyi and Onireti, Oluwakayode and Xu, Hao and Zhang, Lei and Imran, Muhammad},
title = {AI and Blockchain Enabled Future Wireless Networks: A Survey And Outlook},
year = {2024},
issue_date = {September 2024},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {3},
number = {3},
url = {https://doi.org/10.1145/3644369},
doi = {10.1145/3644369},
abstract = {Due to the explosion of mobile users and the ever-increasing heterogeneity and scale of wireless networks, traditional communication protocols and optimizing methods can not satisfy future wireless network (FWN) requirements. As promising technologies, artificial intelligence (AI) and blockchain are deemed as the solution for the FWN. AI, famous for its big data processing ability, will enable the FWN to self-update itself to better adapt to the dynamic network condition. Blockchain, as a distributed ledger, can guarantee data integrity, security, and privacy. In this survey, we overview the concept of AI and blockchain and present their state-of-the-art applications in wireless networks. The potential of AI and blockchain is still huge and waiting to be fully explored in wireless networks. Therefore, we introduce how AI and blockchain can assist each other in FWNs. Furthermore, we explore the current constraints of applying both technologies in the FWNs. In the final part, we discuss the future direction of the deployment of AI and blockchain in FWNs.},
journal = {Distrib. Ledger Technol.},
month = sep,
articleno = {22},
numpages = {30},
keywords = {Artificial intelligence, blockchain, future wireless network (FWN), internet of things, distributed ledger technology (DLT)}
}

@inproceedings{10.1145/3708394.3708427,
author = {Yang, Qiping and Liu, Juan and Zheng, Jingping and Su, Ling},
title = {Explore the Prediction of Formative Assessment to Academic Success in Blended Learning: A Case Study},
year = {2025},
isbn = {9798400710650},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3708394.3708427},
doi = {10.1145/3708394.3708427},
abstract = {The objective of this study is to predict students' final achievement based on their ongoing engagement and formative assessment results within learning management systems (LMSs). This study incorporates ongoing assessment performance as a key factor in measuring student engagement and predicting final exam performance. The results indicate that in the blended teaching process, firstly, including ongoing engagement activities in the final assessment has a positive effect on final exam scores. Secondly, the predictive effect of multiple continuous tasks is superior to that of a single continuous task, and ongoing engagement activities are better predictors of final exam scores than formative assessments. Thirdly, using a standard level as a reference for prediction yields relatively high accuracy, while using an average level as a reference can better identify at-risk students. Therefore, it is recommended that instructors comprehensively incorporate continuous tasks that can reflect students' engagement into the final assessment in blended courses.},
booktitle = {Proceeding of the 2024 International Conference on Artificial Intelligence and Future Education},
pages = {189–196},
numpages = {8},
keywords = {Academic Success, Blended Learning, Formative assessment, Learning Management System, Ongoing Assessment Tasks},
location = {
},
series = {AIFE '24}
}

@inproceedings{10.1145/3613904.3642703,
author = {Agnew, William and Bergman, A. Stevie and Chien, Jennifer and D\'{\i}az, Mark and El-Sayed, Seliem and Pittman, Jaylen and Mohamed, Shakir and McKee, Kevin R.},
title = {The Illusion of Artificial Inclusion},
year = {2024},
isbn = {9798400703300},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3613904.3642703},
doi = {10.1145/3613904.3642703},
abstract = {Human participants play a central role in the development of modern artificial intelligence (AI) technology, in psychological science, and in user research. Recent advances in generative AI have attracted growing interest to the possibility of replacing human participants in these domains with AI surrogates. We survey several such “substitution proposals” to better understand the arguments for and against substituting human participants with modern generative AI. Our scoping review indicates that the recent wave of these proposals is motivated by goals such as reducing the costs of research and development work and increasing the diversity of collected data. However, these proposals ignore and ultimately conflict with foundational values of work with human participants: representation, inclusion, and understanding. This paper critically examines the principles and goals underlying human participation to help chart out paths for future work that truly centers and empowers participants.},
