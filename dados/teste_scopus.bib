	keywords = {Adversarial machine learning; Contrastive Learning; Scalability; Self-supervised learning; Adaptive trust; Decentralised; Intelligent devices; Machine learning techniques; Machine-learning; New approaches; Resource limitations; Self- adaptations; Social internet of thing; Trust management; Federated learning},
