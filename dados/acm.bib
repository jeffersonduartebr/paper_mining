@inproceedings{10.1145/3708394.3708454,
author = {Li, Hefeifan and Xu, Lixin},
title = {Personalised Learning Methodologies for University English with the Application of Artificial Intelligence},
year = {2025},
isbn = {9798400710650},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3708394.3708454},
doi = {10.1145/3708394.3708454},
abstract = {Given the increasing population of English learners, a number of well-optimized learning approaches greatly improve learners’ overall academic performance. The Artificial Intelligence (AI) technology has contributed to the rapid development of education, and it has also had a beneficial impact on English learning. To examine the favourable effect in the process of learning English at university, we have extensively incorporated AI technology, including building and optimizing learning paths with the help of AI performance evaluation approaches, optimising the organisation and use of learning resources with utilization of AI search, file linking, and file management tools, etc., and gained some practical experience, which is summarised as “personalised learning methodologies for university English with the application of Artificial Intelligence”, which is abbreviated as “PLM-UE-A+AI”, and further shortened as “PAI”. In demonstrating our work, we firstly analyse the characteristics and difficulties of university English learning, and identify its main difficulties as: the improvement of learning objectives, the extension of learning contents, the flexibility and diversity of learning methods and approaches, the limitation of learning time and academic pressure, and the difficulties brought by the introduction of new technologies such as Artificial Intelligence. Then, we present the scientific meaning of PAI, and its scientific content, which includes learning resource construction, learning path planning, learning support services, learning analysis and evaluation, as well as feedback and re-optimisation of learning strategies. Furthermore, we provide a specific implementation plan and specific implementation solutions for the practical application of PAI, by defining and establishing learning objectives, constructing and optimising learning paths, organising learning resources and learning modes, scientific performance evaluation and feedback, and continuously introducing new learning methods. Our practice shows that PAI helps to improve the mastery of grammar and vocabulary and communication endowment, which effectively reduces resistance to learning, strengthens the standardised use of learning resources, and enhances the learning effect.},
booktitle = {Proceeding of the 2024 International Conference on Artificial Intelligence and Future Education},
pages = {352–357},
numpages = {6},
keywords = {Artificial Intelligence, learning strategies, personalised learning},
location = {
},
series = {AIFE '24}
}

